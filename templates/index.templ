package templates

templ Index(text string) {
    @BaseLayout("WOWOWOW") {
        @IndexContent(text)
    }
}

templ IndexContent(text string) {
<div class="container">
	<div class="speech-bubble">
		<h1>{ text }</h1>
	</div>
	<div>
		<img
			src="/static/andoid.png"
			alt="andoid"
			class="logo"
		/>
	</div>
</div>
}